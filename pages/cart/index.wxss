.cart-container{
  width: 100%;
  height: 100%;
  position: relative;
  box-sizing: border-box;
}

.cart-container .empty-data{
  width: 100%;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.cart-cont-scroll{
  width: 100%;
  height: calc(100% - 100rpx);
}

.cart-cont-bot{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  background-color: #f1f1f1;
}

.cc-bot-sel{
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content:center;
}

.bot-sel-img{
  width: 40rpx;
  height: 40rpx;
  margin: 0  25rpx 0 0;
}

.bot-sel-text{
  font-size: 28rpx;
  color: red;
}

.cc-bot-price{
  height: 100%;
  font-size: 28rpx;
  color: red;
}

.cc-bot-sumbmit{
  width: 40%;
  height: 100%;
  text-align: center;
  font-size: 28rpx;
  color: white;
  background-color: red;
}
